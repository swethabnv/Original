<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wsndata.data">
	<class name="CheckingDiseaseChild" table="CheckingDiseaseChild">
		<composite-id>
			<key-property name="diseaseChildId" column="DiseaseChildId" /> 
			<key-property name="checkingDiseaseId" column="CheckingDiseaseId" />
		</composite-id> 
		<property name="description" column="Description" />
		
 	</class>
</hibernate-mapping>