<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wsndata.data">
	<class name="UserType" table="UserType">
		<composite-id>
			<key-property name="userName" column="UserName" />
			<key-property name="userType" column="UserType" />
		</composite-id> 
		
        <property name="lastUpdateDate" column="LastUpdateDate" />
        <property name="lastUpdateBy" column="LastUpdateBy" />
       
 	</class>
</hibernate-mapping>