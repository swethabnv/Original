<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.wsndata.data">
	<class name="ProductionForecast" table="ProductionForecast">
		<composite-id>
			<key-property name="pfYear" column="PFYear" />
			<key-property name="breedTypeId" column="BreedTypeId" />
			<key-property name="breedGroupId" column="BreedGroupId" />
			<key-property name="regionNo" column="RegionNo" />
			<key-property name="provinceNo" column="ProvinceNo" />
		</composite-id> 
	    <property name="irrigationInAmount" column="IrrigationInAmount" />
        <property name="irrigationOutAmount" column="IrrigationOutAmount" />        

 	</class>
</hibernate-mapping>